{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["lib", "storybook-static", "public"]
    },
    "test": {
      "inputs": ["packages/*/src/**/*.tsx", "packages/*/**/*.js"]
    },
    "lint": {},
    "dev": {
      "cache": false
    }
  }
}
